#!/usr/bin/perl -w

use strict;
use warnings;
use Data::Dumper;



my $file = `echo \$DATAHUB/mainDB.seedDB/data_gene_meta.tsv`; chomp $file;

open( IN, "<$file" ) or die "$!\n";

my $header = 0;

open( OUT, ">a.tsv" );

while( <IN> ) {

    chomp $_;
    if( $header == 0) {
	$header++;
	next;
    }

    my @line = split( /\|/, $_ );
    print Dumper \@line;
	
}

close( IN );
close( OUT );
